	rules: 
	mode: core
keyword:
	keyword: Tequilator
	match: tequilator <atom-param>number</atom-param>
	reminder: When {this_or_that("creature", upper:false)} creature attacks, defending player takes {english_number_a(param1)} shot(s).
	rules: 
	mode: expert
keyword:
	keyword: Drinkcycling
	match: drinkcycling <atom-param>number</atom-param>
	reminder: Drink {param1}, discard this card: Draw a card.
	rules: 
	mode: expert
keyword:
	keyword: Redemption counters
	match: redemption counters
	reminder: Remove one redemption counter from your creatures for each drink you take, or fifteen for each chug/shot.
	rules: 
	mode: expert
keyword:
	keyword: Intoxicated
	match: intoxicated
	reminder: This {if is_creaturish(card.type) then "creature" else "source"} deals damage to players in the form of drinks.
	rules: 
	mode: core
keyword:
	keyword: DrunkenCrew
	match: DrunkenCrew <atom-param>number</atom-param>
	reminder: Tap any number of creatures with intoxicated or boozetouch you control with total power {param1} or more: This {separate_words(spacer: " ", card.sub_type)} becomes an artifact creature until end of turn.
	rules: 
	mode: expert
keyword:
	keyword: Boozethirst
	match: Boozethirst <atom-param>number</atom-param>
	reminder: If an opponent has drank this turn, {this_or_that("creature")} creature enters with {english_number_a(param1)} +1/+1 counter(s) on it.
	rules: 
	mode: expert
keyword:
	keyword: Chugtouch
	match: chugtouch
	reminder: When {this_or_that("creature", upper:false)} deals combat damage to a player, they chug a beer.
